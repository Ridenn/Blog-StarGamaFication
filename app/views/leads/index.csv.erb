<%- headers = ['email', 'nome', 'ip', 'tipo', 'data_hora'] -%>
<%= CSV.generate_line headers %>
<%- @leads.each do |lead| -%>
<% if lead.occupation.include? "diretor" %>
  <% lead.kind = "B2B"%>
<% end %>
<%= CSV.generate_line([lead.email, "#{lead.name} #{lead.surname}", lead.ip, lead.kind, lead.created_at.strftime('%Y-%m-%d %T')]) -%>
<%- end -%>
